   <children>
      <ScrollPane hbarPolicy="NEVER" layoutY="87.0" pannable="true" prefHeight="500.0" prefWidth="1075.0" vbarPolicy="ALWAYS">
         <content>
            <ImageView fx:id="ImageView" fitHeight="814.0" fitWidth="1060.0" pickOnBounds="true" preserveRatio="true" />
         </content>
         <cursor>
            <Cursor fx:constant="MOVE" />
         </cursor>
      </ScrollPane>
      <Label fx:id="Name" layoutX="88.0" layoutY="32.0" prefHeight="25.0" prefWidth="921.0" text="Label" textFill="#37366f">
