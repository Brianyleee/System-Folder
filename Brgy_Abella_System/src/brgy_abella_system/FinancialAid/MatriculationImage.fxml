      </ImageView>
      <Button fx:id="ExitBtn" layoutX="1018.0" layoutY="24.0" mnemonicParsing="false" onAction="#ExitBtnAction" prefHeight="25.0" prefWidth="28.0" style="-fx-border-color: transparent; -fx-background-color: transparent;">
